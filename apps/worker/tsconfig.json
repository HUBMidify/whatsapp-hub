{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": ["src/**/*.ts"],
  "exclude": [
    "node_modules",
    "dist",
    "prisma",
    "src/index_BKP.ts",
    "**/*.BKP.ts",
    "**/*_BKP.ts"
  ],
  "scripts": {
  "build": "prisma generate && tsc -p tsconfig.json"
  }
}
